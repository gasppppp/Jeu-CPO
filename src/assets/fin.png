public/14/vs"}})</script><script>window.__SANDBOX_DATA__ = {"like_count":0,"author":{"id":"0a42ea20-0333-416a-afd3-9b74be7f3f8a","name":null,"username":"bdarties","avatar_url":"https:\u002F\u002Favatars.githubusercontent.com\u002Fu\u002F10554240?v=4","personal_workspace_id":"bc48a036-da74-4525-9aa6-fef8921262d5","subscription_plan":null,"subscription_since":null},"id":"y4mkyr","restrictions":{"free_plan_editing_restricted":false,"live_sessions_restricted":true},"picks":[],"team":{"id":"bc48a036-da74-4525-9aa6-fef8921262d5","name":"bdarties","settings":{"ai_consent":{"public_sandboxes":false,"private_sandboxes":false}},"subscription_type":null,"avatar_url":"https:\u002F\u002Favatars1.githubusercontent.com\u002Fu\u002F10554240?v=4"},"forked_template":{"id":"0ab61b40-df89-49a1-a864-df8e781fa762","title":"phaser-base-plateforme","color":"#61DAFB","v2":false,"url":null,"published":false,"icon_url":null,"official":false},"alias":"06-click-4-shoot-y4mkyr","authorization":"read","modules":[{"code":"{\n  \"name\": \"sae301-click-4-shoot\",\n  \"version\": \"1.0.0\",\n  \"description\": \"\",\n  \"main\": \"index.html\",\n  \"scripts\": {\n    \"start\": \"parcel index.html --open\",\n    \"build\": \"parcel build index.html\"\n  },\n  \"dependencies\": {\n    \"phaser\": \"3.55.2\"\n  },\n  \"devDependencies\": {\n    \"@babel\u002Fcore\": \"7.2.0\",\n    \"typescript\": \"4.4.4\"\n  },\n  \"resolutions\": {\n    \"@babel\u002Fpreset-env\": \"7.13.8\"\n  },\n  \"keywords\": []\n}","id":"f5c58d27-6540-4614-bc07-f223bf74b47e","is_binary":false,"title":"package.json","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-08T21:30:06","upload_id":null,"shortid":"ZGQK6","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":null},{"code":"\u003C!DOCTYPE html\u003E\n\u003Chtml\u003E\n  \u003Chead\u003E\n    \u003Ctitle\u003EMon premier jeu en Phaser 3\u003C\u002Ftitle\u003E\n    \u003Cmeta charset=\"UTF-8\" \u002F\u003E\n  \u003C\u002Fhead\u003E\n\n  \u003Cbody\u003E\n    \u003Cscript src=\"src\u002Fjs\u002Findex.js\"\u003E\u003C\u002Fscript\u003E\n  \u003C\u002Fbody\u003E\n\u003C\u002Fhtml\u003E\n","id":"bb13bd72-0af7-4d5c-b16b-370f7a489b7c","is_binary":false,"title":"index.html","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-04T07:29:34","upload_id":null,"shortid":"MWN4P","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":null},{"code":"{\n  \"responsive-preview\": {\n    \"Mobile\": [\n      320,\n      675\n    ],\n    \"Tablet\": [\n      1024,\n      765\n    ],\n    \"Desktop\": [\n      1400,\n      800\n    ],\n    \"Desktop  HD\": [\n      1920,\n      1080\n    ]\n  }\n}","id":"054bc68d-3172-4ee0-9d79-ed1591713b27","is_binary":false,"title":"workspace.json","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2020-11-11T14:58:27","upload_id":null,"shortid":"B1e5M2OFYP","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":"rkcG3_tYP"},{"code":"https:\u002F\u002Fuploads.codesandbox.io\u002Fuploads\u002Fuser\u002F50abf6c9-7f32-4d09-b84e-d64af13ec58c\u002FIfLl-projectile5.png","id":"edc754b1-c7d3-42b7-a16a-43402225495c","is_binary":true,"title":"projectile5.png","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-04T06:58:48","upload_id":null,"shortid":"Bk52FItzi","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":"HJvlnKN3c"},{"code":"import Phaser from \"phaser\";\r\n\r\n\u002F***********************************************************************\u002F\r\n\u002F** VARIABLES GLOBALES \r\n\u002F***********************************************************************\u002F\r\n\r\n\u002F\u002Fvariable du joueur\r\nvar player;\r\nvar haut;\r\nvar bas;\r\nvar gauche;\r\nvar droite;\r\nvar dash;\r\n\r\n\u002F\u002F enemy variable\r\nvar enemy;\r\nvar xCoord;\r\nvar yCoord;\r\nvar elem;\r\nvar obj;\r\n\r\n\u002F\u002Fautres variable\r\nvar clavier;\r\nvar input;\r\nvar mouse;\r\nvar tire = true;\r\nvar cannonball;\r\nvar calque_plateformes;\r\nvar solide;\r\nvar space;\r\nvar score;\r\nvar scoreText;\r\nvar perdu;\r\nvar son_feu;\r\nvar son_gameover;\r\n\u002F***********************************************************************\u002F\r\n\u002F** FONCTIONS GLOBALES \r\n\u002F***********************************************************************\u002F\r\nfunction gameOver(enemy, player) {\r\n  this.scene.start(\"gameOver\");\r\n}\r\n\r\nfunction kill(enemy, cannonball) {\r\n  enemy.destroy(true, true);\r\n  cannonball.destroy(true, true);\r\n  score += 10;\r\n  scoreText.setText(\"Score: \" + score);\r\n}\r\n\r\nfunction createEnemy() {\r\n  xCoord = Math.random() * 1280;\r\n  yCoord = Math.random() * 750;\r\n  if (455 \u003C xCoord && xCoord \u003C 655) {\r\n    xCoord = xCoord - 400;\r\n    obj = this.physics.add.sprite(xCoord, yCoord, \"img_ene\");\r\n  } else if (655 \u003C xCoord && xCoord \u003C 855) {\r\n    xCoord = xCoord + 400;\r\n    obj = this.physics.add.sprite(xCoord, yCoord, \"img_ene\");\r\n  } else if (220 \u003C yCoord && yCoord \u003C 420) {\r\n    yCoord = yCoord - 120;\r\n    obj = this.physics.add.sprite(xCoord, yCoord, \"img_ene\");\r\n  } else if (420 \u003C yCoord && yCoord \u003C 620) {\r\n    yCoord = yCoord + 100;\r\n    obj = this.physics.add.sprite(xCoord, yCoord, \"img_ene\");\r\n  } else {\r\n    obj = this.physics.add.sprite(xCoord, yCoord, \"img_ene\");\r\n  }\r\n\r\n  obj.setCollideWorldBounds(false);\r\n  this.physics.add.collider(obj, calque_plateformes);\r\n  this.physics.add.collider(obj, solide);\r\n  enemy.add(obj);\r\n}\r\n\r\nfunction chocBalle(cannonball, calque_plateformes) {\r\n  cannonball.destroy();\r\n}\r\n\r\nexport default class selection extends Phaser.Scene {\r\n  constructor() {\r\n    super({ key: \"selection\" }); \u002F\u002F mettre le meme nom que le nom de la classe\r\n  }\r\n\r\n  \u002F***********************************************************************\u002F\r\n  \u002F** FONCTION PRELOAD \r\n\u002F***********************************************************************\u002F\r\n\r\n  preload() {\r\n    \u002F\u002F tous les assets du jeu sont placés dans le sous-répertoire src\u002Fassets\u002F\r\n    this.load.image(\"img_ciel\", \"src\u002Fassets\u002Fsky.png\");\r\n    this.load.spritesheet(\"img_perso\", \"src\u002Fassets\u002Fcharsheet.png\", {\r\n      frameWidth: 32,\r\n      frameHeight: 48\r\n    });\r\n\r\n    this.load.spritesheet(\"img_ene\", \"src\u002Fassets\u002Fenesheet.png\", {\r\n      frameWidth: 32,\r\n      frameHeight: 48\r\n    });\r\n\r\n    this.load.image(\"bullet\", \"src\u002Fassets\u002Fprojectile5.png\");\r\n    this.load.image(\"Phaser_tuilesdejeu\", \"src\u002Fassets\u002Ftileset.png\");\r\n    this.load.tilemapTiledJSON(\"carte\", \"src\u002Fassets\u002Fmap1.json\");\r\n\r\n    this.load.audio(\"coupDeFeu\", \"src\u002Fassets\u002Ffire.mp3\");\r\n    this.load.audio(\"GameOverSFX\", \"src\u002Fassets\u002Fdeath.mp3\");\r\n  }\r\n\r\n  \u002F***********************************************************************\u002F\r\n  \u002F** FONCTION CREATE \r\n\u002F***********************************************************************\u002F\r\n\r\n  create() {\r\n    score = 0;\r\n    \u002F*************************************\r\n     *  CREATION DU MONDE + PLATEFORMES  *\r\n     *************************************\u002F\r\n\r\n    const carteDuNiveau = this.add.tilemap(\"carte\");\r\n    const tileset = carteDuNiveau.addTilesetImage(\r\n      \"tileset\",\r\n      \"Phaser_tuilesdejeu\"\r\n    );\r\n\r\n    const sol = carteDuNiveau.createLayer(\"sol\", tileset);\r\n\r\n    calque_plateformes = carteDuNiveau.createLayer(\"mur\", tileset);\r\n    solide = carteDuNiveau.createLayer(\"solide\", tileset);\r\n    \u002F\u002F chargement du calque calque_background_2\r\n    const decor = carteDuNiveau.createLayer(\"decor\", tileset);\r\n\r\n    \u002F\u002F chargement du calque calque_plateformes\r\n\r\n    calque_plateformes.setCollisionByProperty({ estSolide: true });\r\n    solide.setCollisionByProperty({ estSolide: true });\r\n    \u002F****************************\r\n     *  CREATION SCORE  *\r\n     ****************************\u002F\r\n\r\n    scoreText = this.add.text(16, 16, \"score: 0\", {\r\n      fontSize: \"32px\",\r\n      fontStyle: \"bold\",\r\n      fill: \"#000\"\r\n    });\r\n\r\n    \u002F****************************\r\n     *  CREATION DU MECHANT  *\r\n     ****************************\u002F\r\n\r\n    enemy = this.physics.add.group();\r\n\r\n    let n = 0;\r\n\r\n    while (n \u003C 5) {\r\n      createEnemy.call(this); \u002F\u002F!!!\r\n      n++;\r\n    }\r\n\r\n    \u002F****************************\r\n     *  CREATION DU PERSONNAGE  *\r\n     ****************************\u002F\r\n\r\n    \u002F\u002F On créée un nouveeau personnage : player\r\n    player = this.physics.add.sprite(655, 420, \"img_perso\");\r\n\r\n    player.setCollideWorldBounds(false);\r\n    this.physics.add.collider(player, calque_plateformes);\r\n    this.physics.add.collider(player, solide);\r\n\r\n    \u002F***************************\r\n     *  CREATION DES ANIMATIONS *\r\n     ****************************\u002F\r\n\r\n    \u002F\u002Fanimation dos\r\n    this.anims.create({\r\n      key: \"anim_dos\",\r\n      frames: this.anims.generateFrameNumbers(\"img_perso\", {\r\n        start: 8,\r\n        end: 11\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation face\r\n    this.anims.create({\r\n      key: \"anim_face\",\r\n      frames: this.anims.generateFrameNumbers(\"img_perso\", {\r\n        start: 12,\r\n        end: 15\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation gauche\r\n    this.anims.create({\r\n      key: \"anim_gauche\",\r\n      frames: this.anims.generateFrameNumbers(\"img_perso\", {\r\n        start: 0,\r\n        end: 3\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation droite\r\n    this.anims.create({\r\n      key: \"anim_droite\",\r\n      frames: this.anims.generateFrameNumbers(\"img_perso\", {\r\n        start: 4,\r\n        end: 7\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation pause\r\n    this.anims.create({\r\n      key: \"anim_pause\",\r\n      frames: [{ key: \"img_perso\", frame: 3 }],\r\n      frameRate: 20\r\n    });\r\n\r\n    \u002F***************************\r\n     *  CREATION DES ANIMATIONS DES ENNEMIS *\r\n     ****************************\u002F\r\n\r\n    \u002F\u002Fanimation dos\r\n    this.anims.create({\r\n      key: \"anim_dos_ene\",\r\n      frames: this.anims.generateFrameNumbers(\"img_ene\", {\r\n        start: 8,\r\n        end: 11\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation face\r\n    this.anims.create({\r\n      key: \"anim_face_ene\",\r\n      frames: this.anims.generateFrameNumbers(\"img_ene\", {\r\n        start: 12,\r\n        end: 15\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation gauche\r\n    this.anims.create({\r\n      key: \"anim_gauche_ene\",\r\n      frames: this.anims.generateFrameNumbers(\"img_ene\", {\r\n        start: 0,\r\n        end: 3\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation droite\r\n    this.anims.create({\r\n      key: \"anim_droite_ene\",\r\n      frames: this.anims.generateFrameNumbers(\"img_ene\", {\r\n        start: 4,\r\n        end: 7\r\n      }),\r\n      frameRate: 10,\r\n      repeat: -1\r\n    });\r\n\r\n    \u002F\u002Fanimation pause\r\n    this.anims.create({\r\n      key: \"anim_pause_ene\",\r\n      frames: [{ key: \"img_ene\", frame: 3 }],\r\n      frameRate: 20\r\n    });\r\n\r\n    \u002F***********************\r\n     *  CREATION DU CLAVIER *\r\n     ************************\u002F\r\n    \u002F\u002F ceci permet de creer un clavier et de mapper des touches, connaitre l'état des touches\r\n    clavier = this.input.keyboard.createCursorKeys();\r\n    haut = this.input.keyboard.addKey(\"Z\");\r\n    bas = this.input.keyboard.addKey(\"S\");\r\n    gauche = this.input.keyboard.addKey(\"Q\");\r\n    droite = this.input.keyboard.addKey(\"D\");\r\n    dash = this.input.keyboard.addKey(\"shift\");\r\n    input = this.input;\r\n    mouse = this.input.mousePointer;\r\n\r\n    space = this.input.keyboard.addKey(\"L\");\r\n    player.modeDash = false;\r\n\r\n    this.physics.add.collider(enemy, enemy);\r\n    this.physics.add.collider(enemy, player, gameOver, null, this);\r\n\r\n    son_feu = this.sound.add(\"coupDeFeu\");\r\n  }\r\n\r\n  \u002F***********************************************************************\u002F\r\n  \u002F** FONCTION UPDATE \r\n\u002F***********************************************************************\u002F\r\n\r\n  update() {\r\n    \u002F*************************\r\n     *     ENEMY FOLLOW     *\r\n     ************************\u002F\r\n\r\n    elem = enemy.getChildren();\r\n    var p = elem.length;\r\n    let n = 0;\r\n    while (n \u003C p) {\r\n      this.physics.moveTo(elem[n], player.x, player.y, 80);\r\n\r\n      if (\r\n        elem[n].body.velocity.x \u003E 0 &&\r\n        elem[n].body.velocity.x \u003E elem[n].body.velocity.y\r\n      ) {\r\n        elem[n].anims.play(\"anim_droite_ene\", true);\r\n      } else if (\r\n        elem[n].body.velocity.x \u003C 0 &&\r\n        elem[n].body.velocity.x \u003E elem[n].body.velocity.y\r\n      ) {\r\n        elem[n].anims.play(\"anim_dos_ene\", true);\r\n      } else if (\r\n        elem[n].body.velocity.x \u003C 0 &&\r\n        elem[n].body.velocity.x \u003C elem[n].body.velocity.y\r\n      ) {\r\n        elem[n].anims.play(\"anim_gauche_ene\", true);\r\n      } else {\r\n        elem[n].anims.play(\"anim_face_ene\", true);\r\n      }\r\n\r\n      n++;\r\n    }\r\n\r\n    \u002F*************************\r\n     *    TOUCHES PLAYER     *\r\n     ************************\u002F\r\n\r\n    if (haut.isDown) {\r\n      if (Phaser.Input.Keyboard.JustDown(dash)) {\r\n        player.modeDash = true;\r\n        player.setVelocityY(-700);\r\n        this.time.addEvent({\r\n          delay: 200,\r\n          callback: function () {\r\n            player.modeDash = false;\r\n          },\r\n          callbackScope: this\r\n        });\r\n      }\r\n      if (player.modeDash == false) player.setVelocityY(-160);\r\n      player.direction = \"up\";\r\n      player.anims.play(\"anim_dos\", true);\r\n    } else if (bas.isDown) {\r\n      if (Phaser.Input.Keyboard.JustDown(dash)) {\r\n        player.modeDash = true;\r\n        player.setVelocityY(700);\r\n        this.time.addEvent({\r\n          delay: 200,\r\n          callback: function () {\r\n            player.modeDash = false;\r\n          },\r\n          callbackScope: this\r\n        });\r\n      }\r\n      if (player.modeDash == false) player.setVelocityY(160);\r\n      player.direction = \"down\";\r\n      player.anims.play(\"anim_face\", true);\r\n    } else if (gauche.isDown) {\r\n      if (Phaser.Input.Keyboard.JustDown(dash)) {\r\n        player.modeDash = true;\r\n        player.setVelocityX(-700);\r\n        this.time.addEvent({\r\n          delay: 200,\r\n          callback: function () {\r\n            player.modeDash = false;\r\n          },\r\n          callbackScope: this\r\n        });\r\n      }\r\n      if (player.modeDash == false) player.setVelocityX(-160);\r\n      player.direction = \"left\";\r\n      player.anims.play(\"anim_gauche\", true);\r\n    } else if (droite.isDown) {\r\n      if (Phaser.Input.Keyboard.JustDown(dash)) {\r\n        player.modeDash = true;\r\n        player.setVelocityX(700);\r\n        this.time.addEvent({\r\n          delay: 200,\r\n          callback: function () {\r\n            player.modeDash = false;\r\n          },\r\n          callbackScope: this\r\n        });\r\n      }\r\n      if (player.modeDash == false) player.setVelocityX(160);\r\n      player.direction = \"right\";\r\n      player.anims.play(\"anim_droite\", true);\r\n    } else {\r\n      player.setVelocity(0);\r\n      player.anims.play(\"anim_pause\", true);\r\n    }\r\n\r\n    if (mouse.isDown && tire == true) {\r\n      \u002F\u002Fballe\r\n      cannonball = this.physics.add.sprite(player.x, player.y, \"bullet\");\r\n      son_feu.play();\r\n      this.physics.moveTo(\r\n        cannonball,\r\n        this.input.activePointer.worldX,\r\n        this.input.activePointer.worldY,\r\n        500\r\n      );\r\n      this.physics.add.collider(\r\n        cannonball,\r\n        calque_plateformes,\r\n        chocBalle,\r\n        null,\r\n        this\r\n      );\r\n      this.physics.add.collider(enemy, cannonball, kill, null, this);\r\n\r\n      tire = false;\r\n\r\n      this.time.addEvent({\r\n        delay: 400,\r\n        callback: function () {\r\n          tire = true;\r\n        },\r\n        callbackScope: this\r\n      });\r\n    }\r\n\r\n    if (score == 50) {\r\n      this.scene.start(\"transition2\");\r\n    }\r\n    \u002F*************************\r\n     *     MOUV MECHANT      *\r\n     ************************\u002F\r\n  }\r\n}\r\n","id":"b096c4d8-4928-4079-84e5-dc8e2090b578","is_binary":false,"title":"niveau1.js","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-07T08:05:57","upload_id":null,"shortid":"rNyEB","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":"3Pgvx"},{"code":"https:\u002F\u002Fuploads.codesandbox.io\u002Fuploads\u002Fuser\u002F50abf6c9-7f32-4d09-b84e-d64af13ec58c\u002FOfXC-charsheet.png","id":"8fa4d544-c4f4-48ce-b456-5e75683b1f0a","is_binary":true,"title":"charsheet.png","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-04T14:04:03","upload_id":null,"shortid":"HyVDThtMi","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":"HJvlnKN3c"},{"code":"https:\u002F\