ransition4.png","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-07T11:03:31","upload_id":null,"shortid":"H1MFEdaGj","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":"HJvlnKN3c"},{"code":"https:\u002F\u002Fuploads.codesandbox.io\u002Fuploads\u002Fuser\u002F950b6432-6035-49a3-8952-0f25c76866e6\u002FgVGM-menu.mp3","id":"d81c6425-0c99-4a42-8ae6-e73573a17474","is_binary":true,"title":"menu.mp3","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-07T12:01:10","upload_id":null,"shortid":"rk0ZS5aMi","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":"HJvlnKN3c"},{"code":"import Phaser from \"phaser\";\r\n\r\nvar bouton_play;\r\nvar son_start;\r\nvar musique_menu;\r\n\r\nexport default class menu extends Phaser.Scene {\r\n  constructor() {\r\n    super({ key: \"menu\" });\r\n  }\r\n  \u002F\u002Fon charge les images\r\n  preload() {\r\n    this.load.image(\"menu_fond\", \"src\u002Fassets\u002Fbackground.png\");\r\n    this.load.image(\"imageBoutonPlay\", \"src\u002Fassets\u002Fbouton_up.png\");\r\n    this.load.image(\"boutonPlay\", \"src\u002Fassets\u002Fbouton_down.png\");\r\n    this.load.audio(\"start\", \"src\u002Fassets\u002Fstart.mp3\");\r\n    this.load.audio(\"menu_musique\", \"src\u002Fassets\u002Fmenu.mp3\");\r\n  }\r\n\r\n  create() {\r\n    \u002F\u002F on place les éléments de fond\r\n    this.add.image(0, 0, \"menu_fond\").setOrigin(0).setDepth(0);\r\n    \u002F\u002Fajout de la musique pour le menu\r\n    musique_menu = this.sound.add(\"menu_musique\");\r\n    musique_menu.play();\r\n    \u002F\u002Fon ajoute un bouton de clic, nommé bouton_play\r\n    bouton_play = this.add.image(640, 600, \"imageBoutonPlay\").setDepth(1);\r\n\r\n    \u002F\u002F=========================================================\r\n    \u002F\u002Fon rend le bouton interratif\r\n    bouton_play.setInteractive();\r\n\r\n    \u002F\u002FCas ou la souris passe sur le bouton play\r\n    bouton_play.on(\"pointerover\", () =\u003E {\r\n      \u002F\u002Fbouton_play.disableBody(true, true);\r\n      \u002F\u002Fbouton_play = this.add.image(640, 600, \"boutonPlay\").setDepth(1);\r\n    });\r\n\r\n    \u002F\u002FCas ou la souris ne passe plus sur le bouton play\r\n    bouton_play.on(\"pointerout\", () =\u003E {});\r\n\r\n    \u002F\u002FCas ou la sourris clique sur le bouton play :\r\n    \u002F\u002F on lance le niveau 1\r\n    bouton_play.on(\"pointerup\", () =\u003E {\r\n      bouton_play.destroy(true, true);\r\n      bouton_play = this.add.image(640, 600, \"boutonPlay\").setDepth(1);\r\n      this.time.addEvent({\r\n        delay: 200,\r\n        callback: function () {\r\n          this.scene.start(\"transition1\");\r\n          musique_menu.stop();\r\n          \u002F\u002Fajout de la musique dans le bouton play\r\n          son_start = this.sound.add(\"start\");\r\n          son_start.play();\r\n        },\r\n        callbackScope: this\r\n      });\r\n    });\r\n  }\r\n}\r\n","id":"12ede6c2-9dbe-411b-acc7-4620e636d5ac","is_binary":false,"title":"menu.js","sha":null,"inserted_at":"2022-10-08T21:29:46","updated_at":"2022-10-07T12:03:32","upload_id":null,"shortid":"jvJyR","source_id":"b29feaa5-5234-46a6-b893-7078ad7a225d","directory_shortid":"3Pgvx"},{"code":"import Phaser from \"phaser\";\r\n\r\nexport default class transition4 extends Phaser.Scene {\r\n  constructor() {\r\n    super({ key: \"transition4\" });\r\n  }\r\n  \u002F\u002Fon charge les images\r\n  preload() {\r\n    this.load.image(\"transition4\", \"src\u002Fassets\u002Ftransition4.png\");\r\n  }\r\n\r\n  create() {\r\n    \u002F\u002F on place les éléments de fond\r\n    this.add.image(0, 0, \"transition4\").setOrigin(0).setDepth(0);\r\n  }\r\n\r\n  update() {\r\n    this.time.addEvent({\r\n      delay: 2000,\r\n      callback: function () {\r\n        this.scene.start(\"sans\");\r\n      },\r\n      callbackScope: this\r\n    });\r\n  }\r\n}\r\n","id":"b29ff34a-1f40-452f-9419-dcf6f3a911b8","is_binary":fa